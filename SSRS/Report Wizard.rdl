  <DataSets>
    <DataSet Name="MoviesAndDirectors">
      <Query>
        <DataSourceName>Movies</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@DirectorFilter">
            <Value>=Parameters!DirectorFilter.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT TOP 5
    Film.Title AS Title,
    Film.ReleaseDate,
    Film.RunTimeMinutes,
    Film.OscarWins,
    Director.FullName,
    Director.DoB
FROM
    Director
    INNER JOIN Film
        ON Director.DirectorID = Film.DirectorID
WHERE
    Director.FullName = @DirectorFilter
ORDER BY Film.OscarWins DESC</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Title">
          <DataField>Title</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ReleaseDate">
          <DataField>ReleaseDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="RunTimeMinutes">
          <DataField>RunTimeMinutes</DataField>
          <rd:TypeName>System.Int16</rd:TypeName>
        </Field>
        <Field Name="OscarWins">
          <DataField>OscarWins</DataField>
          <rd:TypeName>System.Byte</rd:TypeName>
        </Field>
        <Field Name="FullName">
          <DataField>FullName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DoB">
          <DataField>DoB</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DirectorList">
      <Query>
        <DataSourceName>Movies</DataSourceName>
        <CommandText>SELECT DISTINCT Director.FullName
FROM Director
INNER JOIN Film
    ON Director.DirectorID = Film.DirectorID
WHERE Film.OscarWins &gt; 0
ORDER BY Director.FullName</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="FullName">
          <DataField>FullName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
